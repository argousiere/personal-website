{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "color": {
      "type": "string",
      "regex": "#([a-fA-F0-9]){6}"
    },
    "slug": {
      "type": "uid",
      "required": true,
      "targetField": "title"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    }
  }
}
